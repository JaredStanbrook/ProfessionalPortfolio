{
  "name": "professionalportfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "module": "server/index.ts",
  "scripts": {
    "dev": "bunx concurrently \"bun run dev:frontend\" \"bun run dev:worker\"",
    "dev:frontend": "vite",
    "dev:worker": "bunx wrangler dev --config wrangler.jsonc --env staging",
    "build": "bun run gen && bun run build:frontend",
    "build:frontend": "tsc -b && vite build",
    "gen": "bunx drizzle-kit generate && bunx wrangler types",
    "migrate:local": "bunx wrangler d1 migrations apply professional-portfolio-db",
    "migrate:remote": "bunx wrangler d1 migrations apply professional-portfolio-db --remote",
    "deploy:staging": "bun run migrate:remote && bunx wrangler deploy --env staging",
    "deploy:prod": "bun run migrate:remote && bunx wrangler deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@googlemaps/extended-component-library": "^0.6.13",
    "@hono/zod-validator": "^0.4.3",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@tanstack/react-form": "^0.33.0",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/react-router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@tanstack/zod-form-adapter": "^0.33.0",
    "@types/mdast": "^4.0.4",
    "@vis.gl/react-google-maps": "^1.5.2",
    "caniuse-lite": "^1.0.30001718",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
