    "hono": "^4.7.9",
    "lucia": "^3.2.2",
    "lucide-react": "^0.510.0",
    "next-themes": "^0.3.0",
    "oslo": "^1.2.1",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-remark": "^2.1.0",
    "react-router-devtools": "^1.1.10",
    "recharts": "^2.15.3",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
