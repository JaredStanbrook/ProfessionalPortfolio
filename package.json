{
  "name": "professionalportfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx concurrently \"bun run dev:frontend\" \"bun run dev:worker\"",
    "dev:frontend": "bunx --bun vite",
    "dev:worker": "bunx wrangler dev --live-reload --env staging",
    "build": "(tsc -b || echo 'TypeScript errors found, continuing with build...') && bunx --bun vite build",
    "gen": "bunx drizzle-kit generate && bunx wrangler types",
    "migrate:local": "bun run gen && bunx wrangler d1 migrations apply professional-portfolio-db",
    "migrate:remote": "bun run gen && bunx wrangler d1 migrations apply professional-portfolio-db --remote",
    "deploy:staging": "bun run migrate:remote && bun run build && bunx wrangler deploy --env staging",
    "deploy:prod": "bun run migrate:remote && bun run build && bunx wrangler deploy",
    "lint": "eslint .",
    "preview": "bun run build:staging && vite preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-form": "^1.19.1",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.131.7",
    "@tanstack/react-router-devtools": "^1.131.7",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.9.0",
    "lucia": "^3.2.2",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "wrangler": "^4.28.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/router-plugin": "^1.131.7",
    "@types/node": "^22.17.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}