{
  "name": "professionalportfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx concurrently \"bun run dev:frontend\" \"bun run dev:worker\"",
    "dev:frontend": "bunx --bun vite",
    "dev:worker": "bunx wrangler dev --live-reload",
    "build": "(tsc -b || echo 'TypeScript errors found, continuing with build...') && bunx --bun vite build",
    "gen": "bunx wrangler types",
    "migrate:local": "bunx drizzle-kit generate && bunx wrangler d1 migrations apply professional-portfolio-db --local",
    "migrate:remote": "bunx drizzle-kit generate && bunx wrangler d1 migrations apply professional-portfolio-db --remote",
    "create-admin:local": "bun run scripts/create-admin.ts",
    "create-admin:remote": "bun run scripts/create-admin.ts --remote",
    "deploy": "bunx wrangler types && bun run build && bunx wrangler deploy",
    "lint": "eslint .",
    "preview": "bun run build && vite preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "dependencies": {
    "@hono-rate-limiter/cloudflare": "^0.2.2",
    "@hono/zod-validator": "^0.5.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.10.6",
    "hono-rate-limiter": "^0.4.2",
    "lucia": "^3.2.2",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "otpauth": "^9.4.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.2",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "wrangler": "^4.50.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.10",
    "@eslint/js": "^9.39.1",
    "@simplewebauthn/types": "^12.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/router-plugin": "^1.139.3",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prompts": "^2.4.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^6.4.1",
    "vitest": "~3.2.0"
  }
}