{
  "name": "professionalportfolio",
  "module": "server/index.ts",
  "type": "module",
  "scripts": {
    "dev:all": "bunx concurrently \"bun run dev:frontend\" \"bun run dev\"",
    "dev:frontend": "cd frontend && bun run dev",
    "start": "bunx wrangler dev",
    "dev": "bunx wrangler dev --live-reload",
    "build": "bunx drizzle-kit generate && cd frontend && bun run build",
    "local": "bunx drizzle-kit generate && bunx wrangler d1 migrations apply sheoak_ct",
    "migrate": "bunx wrangler d1 migrations apply sheoak_ct",
    "gen-schema": "bunx drizzle-kit generate",
    "gen-types": "bunx wrangler types",
    "pre-deploy": "bunx wrangler d1 migrations apply sheoak_ct --remote",
    "deploy": "bunx run pre-deploy && wrangler deploy --env production"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/bun": "latest",
    "@types/google.maps": "^3.58.1",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.28.0",
    "wrangler": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@googlemaps/extended-component-library": "^0.6.13",
    "@hono/zod-validator": "^0.4.1",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@vis.gl/react-google-maps": "^1.5.2",
    "caniuse-lite": "^1.0.30001707",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.3",
    "lucia": "^3.2.2",
    "oslo": "^1.2.1",
    "zod": "^3.24.2"
  }
}
